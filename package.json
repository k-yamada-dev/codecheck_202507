{
  "name": "acuagraphy-online",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.12.3",
  "scripts": {
    "__dev": "next dev",
    "dev": "next dev -p 3001",
    "prebuild": "prisma generate",
    "build": "pnpm gen:auto && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:fix:ps": "eslint . --fix --fix-type problem --fix-type suggestion",
    "format": "prettier --write .",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "gen:openapi": "tsx scripts/generate-openapi.ts",
    "gen:client": "tsx scripts/generate-api-client.ts",
    "gen:hooks": "tsx scripts/generate-tanstack-hooks.ts",
    "gen:all": "pnpm gen:openapi && pnpm gen:client && pnpm gen:hooks",
    "gen:auto": "tsx scripts/generate-all-auto.ts",
    "postgen:auto": "prettier --write \"__generated__/**/*.ts\"",
    "gen:validate": "tsx scripts/generate-all-auto.ts validate",
    "gen:full": "tsx scripts/generate-all-with-stubs.ts",
    "gen:stubs": "tsx scripts/generate-route-stubs.ts",
    "gen:stubs-force": "tsx scripts/generate-route-stubs.ts --force",
    "db:deploy": "prisma migrate deploy"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@google-cloud/storage": "^7.15.2",
    "@hookform/resolvers": "^5.1.1",
    "@mui/material": "^7.2.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^6.13.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cuid": "^3.0.0",
    "date-fns": "^4.1.0",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "http-errors": "^2.0.0",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.1.0",
    "lucide-react": "^0.525.0",
    "next": "^15.1.7",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.60.0",
    "react-i18next": "^15.4.1",
    "react-intersection-observer": "^9.16.0",
    "sharp": "^0.34.3",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.5",
    "uuid": "^11.1.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@jest/globals": "^30.0.5",
    "@next/eslint-plugin-next": "^15.0.2",
    "@openapitools/openapi-generator-cli": "^2.21.4",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^9",
    "eslint-config-next": "^15",
    "eslint-config-prettier": "^10",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "prisma": "^6.13.0",
    "prisma-zod-generator": "^1.1.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typed-openapi": "^1.5.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8",
    "zod-to-ts": "^1.2.0"
  },
  "pnpm": {
    "overrides": {
      "tr46": "^5.0.0",
      "whatwg-url": "^14.0.0"
    },
    "patchedDependencies": {
      "prisma-zod-generator@1.1.1": "patches/prisma-zod-generator@1.1.1.patch"
    }
  }
}
