{
  "name": "acuagraphy-online",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.12.3",
  "scripts": {
    "dev": "pnpm -F @acme/web dev",
    "build": "pnpm -r build",
    "start": "pnpm -F @acme/web start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:fix:ps": "eslint . --fix --fix-type problem --fix-type suggestion",
    "format": "prettier --write .",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:deploy": "pnpm -F @acme/db migrate:deploy",
    "db:status": "pnpm -F @acme/db migrate:status",
    "db:seed": "pnpm -F @acme/db seed",
    "lint:all": "pnpm -r --filter './packages/*' --filter './apps/worker' run lint && pnpm -F @acme/web run lint",
    "lint:fix:all": "pnpm -r --filter './packages/*' --filter './apps/worker' run lint:fix && pnpm -F @acme/web run lint:fix",
    "type-check:all": "pnpm -r --filter './apps/*' --filter './packages/*' run type-check",
    "format:all": "pnpm -r --filter './apps/*' --filter './packages/*' run format",
    "dead-exports:web": "ts-prune -p apps/web/tsconfig.json --ignore \"^(middleware\\.ts|next\\.config\\.ts|tailwind\\.config\\.ts|app/.*(page|layout|route)\\.tsx)$\"",
    "dead-exports:contracts": "ts-prune -p packages/contracts/tsconfig.json",
    "dead-exports:db": "ts-prune -p packages/db/tsconfig.json"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@acme/api-client": "workspace:*",
    "@acme/contracts": "workspace:*",
    "@acme/db": "workspace:*",
    "@google-cloud/pubsub": "^5.2.0",
    "@google-cloud/storage": "^7.17.0",
    "@hookform/resolvers": "^3.10.0",
    "@mui/material": "^7.3.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@tanstack/react-table": "^8.21.3",
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/next": "^3.52.1",
    "@ts-rest/react-query": "^3.52.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.525.0",
    "next": "^15.5.2",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.3",
    "react-intersection-observer": "^9.16.0",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "use-debounce": "^10.0.5",
    "uuid": "^11.1.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@jest/globals": "^30.1.2",
    "@next/eslint-plugin-next": "^15.5.2",
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "depcheck": "^1.4.7",
    "dotenv": "^17.2.1",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.2",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.5",
    "typed-openapi": "^1.5.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "zod-to-ts": "^1.2.0"
  },
  "pnpm": {
    "overrides": {
      "zod": "3.23.8",
      "eslint": "^9.33.0",
      "@eslint/js": "^9.13.0"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "next": "^15",
        "eslint": "^9.0.0"
      }
    }
  }
}
